{
  "schemaVersion": 1,
  "id": "qol27",
  "version": "${version}",
  "name": "QoL27",
  "description": "Multiple Quality of Life improvements for Minecraft 1.21.10",
  "authors": [
    "Sebiann"
  ],
  "contact": {
    "email": "code@sebiann.moe",
    "homepage": "https://sebiann.moe",
    "issues": "https://github.com/Sebiann/qol27/issues",
    "sources": "https://github.com/Sebiann/qol27"
  },
  "license": "GPL-3.0",
  "icon": "assets/qol27/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": ["moe.sebiann.qol27.client.QoL27Client"],
    "main": ["moe.sebiann.qol27.QoL27"],
    "modmenu": ["moe.sebiann.qol27.config.ModMenuIntegration" ]
  },
  "mixins": [
    "qol27.mixins.json"
  ],
  "depends": {
    "yet_another_config_lib_v3": ">=${yacl_version}",
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "<=${minecraft_version}"
  },
  "recommends": {
    "modmenu": "*"
  },
  "custom": {
    "mc-publish": {
      "dependencies": [
        "yacl(required){modrinth:1eAoo2KR}",
        "modmenu(recommended){modrinth:mOgUt4GM}",
        "fabric-api(required){modrinth:P7dR8mSH}",
        "fabric-language-kotlin(required){modrinth:Ha28R6CL}"
      ]
    }
  }
}